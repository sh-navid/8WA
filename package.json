{
  "name": "nabotx",
  "displayName": "NaBotX",
  "description": "This VSCode extension helps developers code more efficiently with AI assistance. Currently, it serves as a simple alternative to Copilot or Cursor.",
  "publisher": "sh-navid",
  "version": "0.2.146",
  "icon": "assets/logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/sh-navid/NaBotX"
  },
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "AI"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nabotx.openSettings",
        "title": "NaBotX: Open Settings"
      },
      {
        "command": "nabotx.addToChat",
        "title": "NaBotX: Add to Chat"
      },
      {
        "command": "nabotx.addToChatExplorer",
        "title": "NabotX: Add File/Folder to chat"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "nabotx.openSettings",
          "group": "navigation",
          "when": "editorFocus"
        }
      ],
      "editor/context": [
        {
          "command": "nabotx.addToChat",
          "group": "navigation"
        }
      ],
      "explorer/context": [
        {
          "command": "nabotx.addToChatExplorer",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "sidebar": [
        {
          "id": "nabotxSidePanel",
          "title": "NaBotX",
          "icon": "assets/logo.png"
        }
      ],
      "activitybar": [
        {
          "id": "nabotxActivityBar",
          "title": "NaBotX",
          "icon": "assets/logo.png"
        }
      ]
    },
    "views": {
      "nabotxActivityBar": [
        {
          "type": "webview",
          "id": "nabotxActivityBarView",
          "name": "NaBotX",
          "icon": "assets/logo.png"
        }
      ],
      "nabotxSidePanel": [
        {
          "type": "webview",
          "id": "nabotxSidePanelView",
          "name": "NaBotX",
          "icon": "assets/logo.png"
        }
      ]
    },
    "configuration": {
      "title": "NaBotX Configuration",
      "properties": {
        "nabotx.path": {
          "type": "string",
          "default": "",
          "description": "The path to the LLM server."
        },
        "nabotx.token": {
          "type": "string",
          "default": "",
          "description": "The API token for LLM model."
        },
        "nabotx.model": {
          "type": "string",
          "default": "",
          "description": "The model to use for AI assistance."
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "vscode-test": "^1.6.1"
  }
}
